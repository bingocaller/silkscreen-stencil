    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.3",
    "autoprefixer": "^9.8.6",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^7.1.0",
    "stylelint": "^13.10.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
